<Configuration status="WARN">
  <Appenders>
    <Console name="stdout" target="SYSTEM_OUT">
      <PatternLayout pattern="[%d] %p %m (%c:%L)%n"/>
    </Console>
  </Appenders>

  <Loggers>
     <Logger name="io.confluent.amq.exchange" level="info" additivity="false">
        <AppenderRef ref="stdout"/>
     </Logger>

    <Root level="WARN">
      <AppenderRef ref="stdout"/>
    </Root>
  </Loggers>
</Configuration>

